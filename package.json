    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^25.0.2",
    "@vitest/coverage-v8": "^4.0.16",
    "@vitest/ui": "^4.0.16",
    "commitlint": "^20.2.0",
    "eslint": "^9.39.2",
    "globals": "^16.5.0",
    "husky": "^9.1.7",
